{
  "manifest_version": 1,
  "version": 1,
  "type": "APP",
  "desktop": {
    "js": [
      "js/desktop.js"
    ]
  },
  "icon": "image/people.png",
  "config": {
    "html": "html/config.html",
    "js": [
      "https://js.kintone.com/jquery/3.3.1/jquery.min.js",
      "js/config.js",
      "js/kintone-ui-component.min.js"
    ],
    "css": [
      "css/51-modern-default.css",
      "css/config.css",
      "css/kintone-ui-component.min.css"
    ],
    "required_params": [
      "table_field",
      "user_field"
    ]
  },
  "name": {
    "en": "Table User Count Plug-in"
  },
  "description": {
    "en": "This plug-in counts the number of times a user is listed inside a table, and places that number into a given field when the record data is saved."
  },
  "homepage_url": {
    "en": "https://developer.kintone.io/hc/en-us/articles/115007851447"
  }
}
